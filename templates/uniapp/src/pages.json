{
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uniapp",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8"
  },
  "subPackages": [],
  "easycom": {
    "custom": {
      "^(?!z-paging-refresh|z-paging-load-more)z-paging(.*)": "z-paging/components/z-paging$1/z-paging$1.vue"
    }
  },
  "pages": [
    {
      "path": "pages/index",
      "type": "home",
      "style": {
        "navigationBarTitleText": "首页",
        "disableScroll": false
      }
    },
    {
      "path": "pages/components/index",
      "type": "page",
      "style": {
        "navigationBarTitleText": "组件示例",
        "disableScroll": false
      }
    },
    {
      "path": "pages/hooks/index",
      "type": "page",
      "style": {
        "navigationBarTitleText": "hooks示例",
        "disableScroll": false
      }
    },
    {
      "path": "pages/my/index",
      "type": "page",
      "style": {
        "navigationBarTitleText": "我的",
        "disableScroll": false
      }
    },
    {
      "path": "pages/components/demos/z-padding",
      "type": "page",
      "style": {
        "navigationBarTitleText": "z-padding demo"
      }
    }
  ],
  "tabBar": {
    "color": "#AAA",
    "selectedColor": "#20BD6F",
    "borderStyle": "white",
    "backgroundColor": "rgba(255,255,255,0.3)",
    "fontSize": "12px",
    "height": "56px",
    "list": [
      {
        "pagePath": "pages/index",
        "text": "首页",
        "iconPath": "static/tabbar/home.png",
        "selectedIconPath": "static/tabbar/home-selected.png"
      },
      {
        "pagePath": "pages/components/index",
        "text": "组件",
        "iconPath": "static/tabbar/components.png",
        "selectedIconPath": "static/tabbar/components-selected.png"
      },
      {
        "pagePath": "pages/hooks/index",
        "text": "hooks",
        "iconPath": "static/tabbar/hooks.png",
        "selectedIconPath": "static/tabbar/hooks-selected.png"
      },
      {
        "pagePath": "pages/my/index",
        "text": "我的",
        "iconPath": "static/tabbar/my.png",
        "selectedIconPath": "static/tabbar/my-selected.png"
      }
    ]
  }
}